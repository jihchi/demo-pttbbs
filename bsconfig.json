{
  "$schema": "https://raw.githubusercontent.com/rescript-lang/rescript-compiler/master/docs/docson/build-schema.json",
  "name": "demo-pttbbs",
  "namespace": false,
  "reason": { "react-jsx": 3 },
  "refmt": 3,
  "bs-dependencies": [
    "@rescript/react",
    "@ryyppy/rescript-promise",
    "bs-fetch"
  ],
  "ppx-flags": [],
  "sources": [
    { "dir": "src", "subdirs": true }
  ],
  "package-specs": {
    "module": "es6",
    "in-source": true
  },
  "suffix": ".js",
  "warnings": {
    "error": "+101+8"
  },
  "bsc-flags": [
    "-bs-super-errors",
    "-bs-g",
    "-open Belt"
  ],
  "gentypeconfig": {
    "language": "untyped",
    "shims": [],
    "module": "es6"
  }
}
